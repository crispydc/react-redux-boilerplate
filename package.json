{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Simple React boilerplate project",
  "main": "index.js",
  "author": "crispydc",
  "license": "ISC",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "test": "jest",
    "lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "clean": "rm build/appstyles.css && rm build/bundle.js"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^13.2.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
